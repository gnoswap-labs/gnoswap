### execute transactions

### execute emission transactions
#### staker v2 balance is zero before distribute
gnokey query vm/qeval --data "gno.land/r/gnoswap/gns.BalanceOf(\"g14fclvfqynndp0l6kpyxkpgn4sljw9rr96hz46l\")"
stdout '0 int64'

#### emission distribute
gnokey maketx call -pkgpath gno.land/r/gnoswap/emission -chainid=tendermint_test -func MintAndDistributeGns -gas-fee 1000000ugnot -gas-wanted 100000000 -broadcast test1

#### staker v2 balance is not zero
gnokey query vm/qeval --data "gno.land/r/gnoswap/gns.BalanceOf(\"g14fclvfqynndp0l6kpyxkpgn4sljw9rr96hz46l\")"
stdout '[1-9][0-9]* int64'

### create pool
gnokey maketx call -pkgpath gno.land/r/gnoswap/gns -func Approve -args g126swhfaq2vyvvjywevhgw7lv9hg8qan93dasu8 -args 9223372036854775807 -gas-fee 1000000ugnot -gas-wanted 100000000 -broadcast -chainid=tendermint_test test1
gnokey maketx call -pkgpath gno.land/r/gnoswap/v2/pool -func CreatePool -args "gno.land/r/gnoland/wugnot" -args "gno.land/r/gnoswap/gns" -args 3000 -args 79228162514264337593543950337 -gas-fee 1000000ugnot -gas-wanted 100000000 -broadcast -chainid=tendermint_test test1
stdout '{"key":"poolPath","value":"gno.land/r/gnoland/wugnot:gno.land/r/gnoswap/gns:3000"}'

### pool creation fee getter & setter
gnokey maketx call -pkgpath gno.land/r/gnoswap/v2/pool -func SetPoolCreationFeeByAdmin -args 0 -gas-fee 1000000ugnot -gas-wanted 100000000 -broadcast -chainid=tendermint_test test1
stdout OK

### withdrawal fee getter & setter
gnokey maketx call -pkgpath gno.land/r/gnoswap/v2/pool -func SetWithdrawalFeeByAdmin -args 0 -gas-fee 1000000ugnot -gas-wanted 100000000 -broadcast -chainid=tendermint_test test1
stdout OK

