-- gnomod.toml --
module = "gno.land/r/basic"
gno = "0.9"
-- main.gno --
package basic

import u256 "gno.land/p/gnoswap/uint256"
import i256 "gno.land/p/gnoswap/int256"

func SubInt(cur realm) {
    result := int(1)
    result -= 1
}

func SubInt8(cur realm) {
    result := int8(1)
    result -= 1
}

func SubInt32(cur realm) {
    result := int64(1)
    result -= 1
}

func SubInt64(cur realm) {
    result := int64(1)
    result -= 1
}

func SubInt256(cur realm) {
    result := i256.Zero()
    result = i256.Zero().Sub(result, i256.One())
}

func SubUint(cur realm) {
    result := uint(1)
    result -= 1
}

func SubUint8(cur realm) {
    result := uint8(1)
    result -= 1
}

func SubUint32(cur realm) {
    result := uint32(1)
    result -= 1
}

func SubUint64(cur realm) {
    result := uint64(1)
    result -= 1
}

func SubUint256(cur realm) {
    result := u256.Zero()
    result = u256.Zero().Sub(result, u256.One())
}

var (
    rusultInt int
    resultInt8 int8
    resultInt32 int64
    resultInt64 int64
    resultInt256 *i256.Int
    resultUint uint
    resultUint8 uint8
    resultUint32 uint32
    resultUint64 uint64
    resultUint256 *u256.Uint
)

func SubInt_With_Storage(cur realm) {
    rusultInt -= 1
}

func SubInt8_With_Storage(cur realm) {
    resultInt8 -= 1
}

func SubInt32_With_Storage(cur realm) {
    resultInt32 -= 1
}

func SubInt64_With_Storage(cur realm) {
    resultInt64 -= 1
}

func SubInt256_With_Storage(cur realm) {
    resultInt256 = i256.Zero()
    resultInt256 = i256.Zero().Sub(resultInt256, i256.One())
}

func SubUint_With_Storage(cur realm) {
    resultUint -= 1
}

func SubUint8_With_Storage(cur realm) {
    resultUint8 -= 1
}

func SubUint32_With_Storage(cur realm) {
    resultUint32 -= 1
}

func SubUint64_With_Storage(cur realm) {
    resultUint64 -= 1
}

func SubUint256_With_Storage(cur realm) {
    resultUint256 = u256.Zero()
    resultUint256 = u256.Zero().Sub(resultUint256, u256.One())
}

func Empty(cur realm) {
    return
}
