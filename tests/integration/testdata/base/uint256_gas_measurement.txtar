-- gnomod.toml --
module = "gno.land/r/gnoswap/uint256gas"
gno = "0.9"

-- main.gno --
package uint256gas

import (
	"gno.land/p/gnoswap/uint256"
)

func Empty(cur realm) {
	return
}

// Constructor Methods
func TestNewUint(cur realm) {
	uint256.NewUint(12345)
}

func TestZero(cur realm) {
	uint256.Zero()
}

func TestOne(cur realm) {
	uint256.One()
}

func TestMustFromDecimal(cur realm) {
	uint256.MustFromDecimal("123456789012345678901234567890")
}

func TestMustFromHex(cur realm) {
	uint256.MustFromHex("0x1234567890abcdef")
}

// Arithmetic Operations
func TestAdd(cur realm) {
	x := uint256.NewUint(1000000)
	y := uint256.NewUint(500000)
	z := uint256.Zero()
	z.Add(x, y)
}

func TestAddOverflow(cur realm) {
	x := uint256.MustFromDecimal("115792089237316195423570985008687907853269984665640564039457584007913129639935")
	y := uint256.NewUint(1)
	z := uint256.Zero()
	z.AddOverflow(x, y)
}

func TestSub(cur realm) {
	x := uint256.NewUint(1000000)
	y := uint256.NewUint(300000)
	z := uint256.Zero()
	z.Sub(x, y)
}

func TestSubOverflow(cur realm) {
	x := uint256.NewUint(100)
	y := uint256.NewUint(200)
	z := uint256.Zero()
	z.SubOverflow(x, y)
}

func TestMul(cur realm) {
	x := uint256.NewUint(1000)
	y := uint256.NewUint(2000)
	z := uint256.Zero()
	z.Mul(x, y)
}

func TestMulOverflow(cur realm) {
	x := uint256.MustFromDecimal("115792089237316195423570985008687907853269984665640564039457584007913129639935")
	y := uint256.NewUint(2)
	z := uint256.Zero()
	z.MulOverflow(x, y)
}

func TestDiv(cur realm) {
	x := uint256.NewUint(1000000)
	y := uint256.NewUint(3)
	z := uint256.Zero()
	z.Div(x, y)
}

func TestMod(cur realm) {
	x := uint256.NewUint(1000000)
	y := uint256.NewUint(7)
	z := uint256.Zero()
	z.Mod(x, y)
}

func TestDivMod(cur realm) {
	x := uint256.NewUint(1000000)
	y := uint256.NewUint(7)
	z := uint256.Zero()
	m := uint256.Zero()
	z.DivMod(x, y, m)
}

func TestMulMod(cur realm) {
	x := uint256.NewUint(1000)
	y := uint256.NewUint(2000)
	m := uint256.NewUint(997)
	z := uint256.Zero()
	z.MulMod(x, y, m)
}

func TestExp(cur realm) {
	base := uint256.NewUint(2)
	exp := uint256.NewUint(10)
	z := uint256.Zero()
	z.Exp(base, exp)
}

func TestNeg(cur realm) {
	x := uint256.NewUint(12345)
	z := uint256.Zero()
	z.Neg(x)
}

// Bitwise Operations
func TestAnd(cur realm) {
	x := uint256.NewUint(0xFF00)
	y := uint256.NewUint(0x0F0F)
	z := uint256.Zero()
	z.And(x, y)
}

func TestOr(cur realm) {
	x := uint256.NewUint(0xFF00)
	y := uint256.NewUint(0x0F0F)
	z := uint256.Zero()
	z.Or(x, y)
}

func TestXor(cur realm) {
	x := uint256.NewUint(0xFF00)
	y := uint256.NewUint(0x0F0F)
	z := uint256.Zero()
	z.Xor(x, y)
}

func TestNot(cur realm) {
	x := uint256.NewUint(0xFF00)
	z := uint256.Zero()
	z.Not(x)
}

func TestLsh(cur realm) {
	x := uint256.NewUint(1)
	z := uint256.Zero()
	z.Lsh(x, 10)
}

func TestRsh(cur realm) {
	x := uint256.NewUint(1024)
	z := uint256.Zero()
	z.Rsh(x, 5)
}

// Comparison Operations
func TestEq(cur realm) {
	x := uint256.NewUint(12345)
	y := uint256.NewUint(12345)
	x.Eq(y)
}

func TestLt(cur realm) {
	x := uint256.NewUint(100)
	y := uint256.NewUint(200)
	x.Lt(y)
}

func TestGt(cur realm) {
	x := uint256.NewUint(200)
	y := uint256.NewUint(100)
	x.Gt(y)
}

func TestCmp(cur realm) {
	x := uint256.NewUint(100)
	y := uint256.NewUint(200)
	x.Cmp(y)
}

func TestIsZero(cur realm) {
	x := uint256.Zero()
	x.IsZero()
}

// Conversion Methods
func TestToString(cur realm) {
	x := uint256.NewUint(123456789)
	x.ToString()
}

func TestUint64(cur realm) {
	x := uint256.NewUint(12345)
	x.Uint64()
}

func TestClone(cur realm) {
	x := uint256.NewUint(12345)
	x.Clone()
}

// FullMath Operations
func TestMulDiv(cur realm) {
	a := uint256.NewUint(1000000)
	b := uint256.NewUint(2000000)
	denominator := uint256.NewUint(3)
	uint256.MulDiv(a, b, denominator)
}

func TestMulDivRoundingUp(cur realm) {
	a := uint256.NewUint(1000000)
	b := uint256.NewUint(2000000)
	denominator := uint256.NewUint(3)
	uint256.MulDivRoundingUp(a, b, denominator)
}
