-- gnomod.toml --
module = "gno.land/r/basic"
gno = "0.9"
-- main.gno --
package basic

import u256 "gno.land/p/gnoswap/uint256"
import i256 "gno.land/p/gnoswap/int256"

func MulInt(cur realm) {
    result := int(1)
    result *= 1
}

func MulInt8(cur realm) {
    result := int8(1)
    result *= 1
}

func MulInt32(cur realm) {
    result := int64(1)
    result *= 1
}

func MulInt64(cur realm) {
    result := int64(1)
    result *= 1
}

func MulInt256(cur realm) {
    result := i256.One()
    result = i256.Zero().Mul(result, i256.One())
}

func MulUint(cur realm) {
    result := uint(1)
    result *= 1
}

func MulUint8(cur realm) {
    result := uint8(1)
    result *= 1
}

func MulUint32(cur realm) {
    result := uint32(1)
    result *= 1
}

func MulUint64(cur realm) {
    result := uint64(1)
    result *= 1
}

func MulUint256(cur realm) {
    result := u256.One()
    result = u256.Zero().Mul(result, u256.One())
}

var (
    rusultInt int
    resultInt8 int8
    resultInt32 int64
    resultInt64 int64
    resultInt256 *i256.Int
    resultUint uint
    resultUint8 uint8
    resultUint32 uint32
    resultUint64 uint64
    resultUint256 *u256.Uint
)

func MulInt_With_Storage(cur realm) {
    rusultInt *= 1
}

func MulInt8_With_Storage(cur realm) {
    resultInt8 *= 1
}

func MulInt32_With_Storage(cur realm) {
    resultInt32 *= 1
}

func MulInt64_With_Storage(cur realm) {
    resultInt64 *= 1
}

func MulInt256_With_Storage(cur realm) {
    resultInt256 = i256.One()
    resultInt256 = i256.Zero().Mul(resultInt256, i256.One())
}

func MulUint_With_Storage(cur realm) {
    resultUint *= 1
}

func MulUint8_With_Storage(cur realm) {
    resultUint8 *= 1
}

func MulUint32_With_Storage(cur realm) {
    resultUint32 *= 1
}

func MulUint64_With_Storage(cur realm) {
    resultUint64 *= 1
}

func MulUint256_With_Storage(cur realm) {
    resultUint256 = u256.One()
    resultUint256 = u256.Zero().Mul(resultUint256, u256.One())
}

func Empty(cur realm) {
    return
}
