-- gnomod.toml --
module = "gno.land/r/gnoswap/strconvgas"
gno = "0.9"
-- main.gno --
package strconvgas

import (
	"strconv"
)

func Empty(cur realm) {
	return
}

// Itoa Functions
func Itoa_SmallPositive(cur realm) {
	strconv.Itoa(42)
}

func Itoa_LargePositive(cur realm) {
	strconv.Itoa(9223372036854775807) // max int64
}

func Itoa_Negative(cur realm) {
	strconv.Itoa(-9223372036854775808) // min int64
}

func Itoa_Zero(cur realm) {
	strconv.Itoa(0)
}

// FormatInt Functions
func FormatInt_Base10(cur realm) {
	strconv.FormatInt(1234567890, 10)
}

func FormatInt_Base16(cur realm) {
	strconv.FormatInt(1234567890, 16)
}

func FormatInt_Base2(cur realm) {
	strconv.FormatInt(1234567890, 2)
}

// FormatUint Functions
func FormatUint_Base10(cur realm) {
	strconv.FormatUint(1234567890, 10)
}

func FormatUint_MaxUint64(cur realm) {
	strconv.FormatUint(18446744073709551615, 10) // max uint64
}

// Atoi Functions
func Atoi_SmallNumber(cur realm) {
	strconv.Atoi("42")
}

func Atoi_LargeNumber(cur realm) {
	strconv.Atoi("9223372036854775807")
}

func Atoi_Negative(cur realm) {
	strconv.Atoi("-9223372036854775808")
}

// ParseInt Functions
func ParseInt_Base10(cur realm) {
	strconv.ParseInt("1234567890", 10, 64)
}

func ParseInt_Base16(cur realm) {
	strconv.ParseInt("499602d2", 16, 64)
}

func ParseInt_Base2(cur realm) {
	strconv.ParseInt("1001001100101100000001011010010", 2, 64)
}

// ParseUint Functions
func ParseUint_Base10(cur realm) {
	strconv.ParseUint("1234567890", 10, 64)
}

func ParseUint_MaxUint64(cur realm) {
	strconv.ParseUint("18446744073709551615", 10, 64)
}

// Quote Functions
func Quote_SimpleString(cur realm) {
	strconv.Quote("Hello, World!")
}

func Quote_SpecialChars(cur realm) {
	strconv.Quote("Hello\nWorld\t\"Test\"")
}

// ParseBool Functions
func ParseBool_True(cur realm) {
	strconv.ParseBool("true")
}

func ParseBool_False(cur realm) {
	strconv.ParseBool("false")
}

// FormatBool Functions
func FormatBool_True(cur realm) {
	strconv.FormatBool(true)
}

func FormatBool_False(cur realm) {
	strconv.FormatBool(false)
}
