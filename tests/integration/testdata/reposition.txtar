-- callback_mock.gno --
package swap_wrapper

import (
	"gno.land/r/gnoswap/pool"
)

func WrappedSwap(
    cur realm,
    token0Path string,
    token1Path string,
    fee uint32,
    recipient address,
    zeroForOne bool,
    amountSpecified string,
    sqrtPriceLimitX96 string,
    payer address,
) (string, string) {
    return pool.Swap(
        cross,
        token0Path,
        token1Path,
        fee,
        recipient,
        zeroForOne,
        amountSpecified,
        sqrtPriceLimitX96,
        payer,
        func(cur realm, token0Path, token1Path string) error {
            return nil
        },
    )
}
