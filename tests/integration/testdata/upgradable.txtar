adduserfrom gns_admin 'mnemonic here'

adduser user1
patchpkg "g16a7etgm9z2r653ucl36rj0l2yqcxgrz2jyegzx" $user1_user_addr


gnoland start

gnokey query vm/qeval --data "gno.land/r/gnoswap/pool.GetImplementationPackagePath()"
stdout "gno.land/r/gnoswap/pool/v1"

gnokey maketx call -pkgpath gno.land/r/gnoswap/pool -func UpgradeImpl -args gno.land/r/gnoswap/pool/v3_valid -insecure-password-stdin=true -broadcast=true -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 1000000000 -memo "" gns_admin
stdout OK!

gnokey query vm/qeval --data "gno.land/r/gnoswap/pool.GetImplementationPackagePath()"
stdout "gno.land/r/gnoswap/pool/v3_valid"

# Upgrade to non-exists version
! gnokey maketx call -pkgpath gno.land/r/gnoswap/pool -func UpgradeImpl -args gno.land/r/gnoswap/pool/invalid -insecure-password-stdin=true -broadcast=true -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 1000000000 -memo "" gns_admin
stderr 'version_manager: initializer not found for package path:gno.land/r/gnoswap/pool/invalid'

# Rollback to v1 contract
gnokey maketx call -pkgpath gno.land/r/gnoswap/pool -func UpgradeImpl -args gno.land/r/gnoswap/pool/v1 -insecure-password-stdin=true -broadcast=true -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 1000000000 -memo "" gns_admin
stdout OK!

gnokey query vm/qeval --data "gno.land/r/gnoswap/pool.GetImplementationPackagePath()"
stdout "gno.land/r/gnoswap/pool/v1"

# Attempting to upgrade contract version with an address that lacks upgrade authority
! gnokey maketx call -pkgpath gno.land/r/gnoswap/pool -func UpgradeImpl -args gno.land/r/gnoswap/pool/v3_valid -insecure-password-stdin=true -broadcast=true -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 1000000000 -memo "" $user1_user_addr
stderr 'unauthorized: caller'
