adduserfrom gns_admin 'source bonus chronic canvas draft south burst lottery vacant surface solve popular case indicate oppose farm nothing bullet exhibit title speed wink action roast'

adduser user1
patchpkg "g16a7etgm9z2r653ucl36rj0l2yqcxgrz2jyegzx" $user1_user_addr


gnoland start

gnokey maketx send -send 15000000000ugnot -to $gns_admin_user_addr -broadcast -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 100000000 test1
gnokey maketx addpkg -pkgdir ../../../examples/gno.land/r/gnoswap/scenario/upgrade/implements/v3_valid/staker -pkgpath gno.land/r/gnoswap/staker/v3_valid -gas-fee 1000000ugnot -gas-wanted 100000000 -broadcast -chainid=tendermint_test test1

gnokey query vm/qeval --data "gno.land/r/gnoswap/staker.GetImplementationPackagePath()"
stdout "gno.land/r/gnoswap/staker/v1"

gnokey maketx call -pkgpath gno.land/r/gnoswap/staker -func UpgradeImpl -args gno.land/r/gnoswap/staker/v3_valid -insecure-password-stdin=true -broadcast=true -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 1000000000 -memo "" gns_admin
stdout 'GAS USED:   2[0-9]{7}'
stdout 'STORAGE DELTA:  4613 bytes'
stdout 'TOTAL TX COST:  100461300ugnot'
stdout 'EVENTS:     \[{\"type\":\"ChangeImplementation\",\"attrs\":\[{\"key\":\"previousPackagePath\",\"value\":\"gno.land/r/gnoswap/staker/v1\"},{\"key\":\"newPackagePath\",\"value\":\"gno.land/r/gnoswap/staker/v3_valid\"}\],\"pkg_path\":\"gno.land/p/gnoswap/version_manager\"},{\"bytes_delta\":[0-9]+,\"fee_delta\":{\"denom\":\"ugnot\",\"amount\":[0-9]+},\"pkg_path\":\"gno.land/r/gnoswap/staker\"}\]'

gnokey query vm/qeval --data "gno.land/r/gnoswap/staker.GetImplementationPackagePath()"
stdout "gno.land/r/gnoswap/staker/v3_valid"

# Upgrade to non-exists version
! gnokey maketx call -pkgpath gno.land/r/gnoswap/staker -func UpgradeImpl -args gno.land/r/gnoswap/staker/invalid -insecure-password-stdin=true -broadcast=true -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 1000000000 -memo "" gns_admin
stderr 'version_manager: initializer not found for package path:gno.land/r/gnoswap/staker/invalid'

# Rollback to v1 contract
gnokey maketx call -pkgpath gno.land/r/gnoswap/staker -func UpgradeImpl -args gno.land/r/gnoswap/staker/v1 -insecure-password-stdin=true -broadcast=true -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 1000000000 -memo "" gns_admin
stdout 'GAS USED:   45[0-9]{6}'
stdout 'STORAGE DELTA:  -842 byte'
stdout 'TOTAL TX COST:  99915800ugnot'
stdout 'EVENTS:     \[{\"type\":\"ChangeImplementation\",\"attrs\":\[{\"key\":\"previousPackagePath\",\"value\":\"gno.land/r/gnoswap/staker/v3_valid\"},{\"key\":\"newPackagePath\",\"value\":\"gno.land/r/gnoswap/staker/v1\"}\],\"pkg_path\":\"gno.land/p/gnoswap/version_manager\"},.*\]'

gnokey query vm/qeval --data "gno.land/r/gnoswap/staker.GetImplementationPackagePath()"
stdout "gno.land/r/gnoswap/staker/v1"

# Attempting to upgrade contract version with an address that lacks upgrade authority
! gnokey maketx call -pkgpath gno.land/r/gnoswap/staker -func UpgradeImpl -args gno.land/r/gnoswap/staker/v3_valid -insecure-password-stdin=true -broadcast=true -chainid=tendermint_test -gas-fee 100000000ugnot -gas-wanted 1000000000 -memo "" $user1_user_addr
stderr 'unauthorized: caller'
