package gnft

import (
	"math/rand"
	"testing"
	"time"
)

func TestImageGen(t *testing.T) {
	seed1 := uint64(time.Now().Unix())
	seed2 := uint64(time.Now().UnixNano())
	pcg := rand.NewPCG(seed1, seed2)
	r := rand.New(pcg)

	uri := genImageURI(r)
	if uri == "" {
		t.Error("empty uri")
	}

	expectedUri := `data:image/svg+xml;base64,`
	if uri != expectedUri {
		t.Errorf("expected %s, got %s", expectedUri, uri)
	}
}
